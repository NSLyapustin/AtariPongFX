<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<GridPane fx:controller="controllers.StartController"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <ImageView id="AtariPongLogo"
               fx:id="boximage"
               disable="false"
               fitHeight="180" fitWidth="520"
               layoutX="69.0" layoutY="300.0"
               pickOnBounds="true"
               preserveRatio="true">
        <image>
            <Image url="@../img/AtariPongLogo.jpg"/>
        </image>
    </ImageView>
    <Button fx:id="startBtn" maxHeight="40" maxWidth="80" layoutX="200" layoutY="200" mnemonicParsing="false" onAction="#startGame" text="Start" />
</GridPane>